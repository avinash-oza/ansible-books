set nocompatible
set number
set autoindent
set expandtab
set shiftwidth=4
set softtabstop=4
set whichwrap+=h,l,<,>,[,]
syntax enable
set backspace=indent,eol,start
set hlsearch
autocmd InsertEnter * set number
autocmd InsertEnter * colorscheme molokai
autocmd InsertEnter * set background=dark
set timeoutlen=0
set ruler
set t_Co=256
colorscheme molokai
set background=dark
"let g:molokai_original=1
